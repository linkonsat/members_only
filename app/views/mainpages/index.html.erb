<%= render "partials/top_nav" %>
<div class="posts_display_container">
<div>
<% if user_signed_in? %>
<%= link_to "create post", url = "posts/new" %> 
<% end %>
</div>
<div class="all_posts_container">
<ul>
<% @user.each do |user| %>
    <li>
    <% if user_signed_in? %>
    <%= user.user_name %>
    <% else %>
    Anonymous
    <% end %>
    </li>
    <% user.posts.each do |post| %>
        <li>
        <%= post.title %>
        </li>
        <% end %>
    <% end %>
    </ul>
    </div>
</div>
<%= render "partials/bottom_nav" %>